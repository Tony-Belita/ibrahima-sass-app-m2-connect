{
  "name": "ibrahima-sass-app-m2-connect",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "prisma:generate": "dotenv -e .env.local -- prisma generate",
    "prisma:push": "dotenv -e .env.local -- prisma db push",
    "prisma:studio": "dotenv -e .env.local -- prisma studio",
    "prisma:migrate": "dotenv -e .env.local -- prisma migrate dev",
    "prisma:reset": "dotenv -e .env.local -- prisma migrate reset",
    "email": "email dev --dir src/app/emails",
    "docker:build": "docker build -t saas-facturation .",
    "docker:run": "docker run -p 3000:3000 saas-facturation",
    "docker:compose": "docker-compose up --build",
    "prepare": "husky install"
  },
  "dependencies": {
    "@clerk/nextjs": "^6.30.0",
    "@neondatabase/serverless": "^1.0.1",
    "@prisma/client": "^6.13.0",
    "@react-email/components": "0.5.0",
    "@tabler/icons-react": "^3.34.1",
    "@tailwindcss/cli": "^4.1.11",
    "@types/bcryptjs": "^2.4.6",
    "bcryptjs": "^3.0.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dotenv": "^17.2.1",
    "drizzle-orm": "^0.33.0",
    "lucide-react": "^0.539.0",
    "motion": "^12.23.12",
    "next": "15.4.6",
    "next-secure-headers": "^2.2.0",
    "prisma": "^6.13.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-email": "4.2.8",
    "react-to-print": "^3.1.1",
    "resend": "^6.0.1",
    "sweetalert2": "^11.22.3",
    "tailwind-merge": "^3.3.1",
    "zod": "^4.0.17"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4.1.11",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/supertest": "^6.0.3",
    "autoprefixer": "^10.4.21",
    "dotenv-cli": "^7.4.2",
    "drizzle-kit": "^0.31.4",
    "eslint": "^9",
    "eslint-config-next": "15.4.6",
    "jest": "^30.0.5",
    "jest-environment-jsdom": "^30.0.5",
    "postcss": "^8.5.6",
    "supertest": "^7.1.4",
    "tailwindcss": "^4.1.11",
    "ts-jest": "^29.4.1",
    "tw-animate-css": "^1.3.6",
    "typescript": "^5"
  }
}
